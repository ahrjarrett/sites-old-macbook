#footer.footer.row
	.footer-top
		ul.footer-ul
			each val, index in footerUl
				li.footer-li
					a(href='/#{val}') #{index}

	hr(style='border-color: #666')
	.footer-bottom
		.copyright
			p Â© 2016 #{company} 
		.social
			p Follow us!
				each val, index in social
					a(href='#{index}')
						i(class="fa fa-#{val}")

include ./modal
